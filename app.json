{
  "expo": {
    "name": "PictureApp",
    "slug": "PictureApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/clones.png",
    "androidStatusBar": {
      "backgroundColor":  "#f3f8ff",
      "translucent": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff" 
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.expo.camerapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "io.expo.camerapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
